name: dockercompose18445631850549716727
services:
  globalshopping:
    build:
      context: D:\Zeeshan\Projects\github\lut\data-intensive\GlobalShopping\GlobalShopping
      dockerfile: GlobalShopping/Dockerfile
    depends_on:
      mongodb:
        condition: service_started
        required: true
      sqlserverAsia:
        condition: service_started
        required: true
      sqlserverEurope:
        condition: service_started
        required: true
      sqlserverUSA:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: globalshopping
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Devel\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Devel\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mongodb:
    image: mongo:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
  sqlserverAsia:
    container_name: sqlserverAsia
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1435
        published: "1435"
        protocol: tcp
  sqlserverEurope:
    container_name: sqlserverEurope
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1434
        published: "1434"
        protocol: tcp
  sqlserverUSA:
    container_name: sqlserverUSA
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
networks:
  default:
    name: dockercompose18445631850549716727_default